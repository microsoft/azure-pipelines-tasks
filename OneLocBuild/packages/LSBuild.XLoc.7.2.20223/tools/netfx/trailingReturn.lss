							<optionSet id="AutoRule 01" displayName="AutoRule 01" helpText="Use these settings to specify an automatic LocVer instruction.">
								<option id="\LocVer\AutoCommenting\AutoRule01\Instruction" displayName="Instruction" helpText="Enter the instruction that will be automatically applied if all conditions are met. Backreferences from the 'Expression' setting are replaced in the instruction.">
									<string defaultValue="" currentValue="{StrEnds=&quot;
&quot;}" />
								</option>
								<option id="\LocVer\AutoCommenting\AutoRule01\RegEx" displayName="Expression" helpText="Enter a regular expression to identify information or characters in the string.">
									<string defaultValue="" currentValue="\n$" />
								</option>
								<option id="\LocVer\AutoCommenting\AutoRule01\Enabled" displayName="Enabled" helpText="To validate strings according to this automatic instruction, select On. To stop validation with this instruction, select Off.">
									<boolean defaultValue="0" currentValue="1" />
								</option>
							</optionSet>
						</optionSet>
					</optionSet>
				</optionSet>
			</optionSet>
		</optionSet>
