    <formathandle type="end" include="yes"></formathandle>
    <formathandle type="isolated" include="yes"></formathandle>
  </header>

  <body>

    <languagerules>

      <!-- Default -->
      <languagerule languagerulename="Default">
        <rule break="no">
          <!--Don't break inside of comments to avoid comment tag in the first segment
              and a closing comment tag in a later segment (invalid XML).        
              
              * Note - using "[\s\S]" instead of "." to match any character b/c 
                       "." matches everything except \n and we need to match \n 
                       (see bug 8586)
          -->
          <beforebreak>&lt;!--[\s\S]*?</beforebreak>
          <afterbreak>--&gt;</afterbreak>
        </rule>
        <rule break="no">
          <!--Don't break inside of cdata to avoid cdata tag in the first segment
              and a closing cdata tag in a later segment (invalid XML). -->
          <beforebreak>&lt;!\[CDATA\[[\s\S]*?</beforebreak>
          <afterbreak>\]\]&gt;</afterbreak>
        </rule>
        <rule break="no">
          <!--Don't break on any text that occurs between inline tags to avoid
              getting an opening tag in the first segment and  a closing tag in 
              a later segment (invalid XML).
              
              E.g. Sentence <ui>one. Sentence two.</ui>  
                - W/O rule ==> Seg 1: Sentence <ui>one., Seg 2: Sentence two.</ui>
                - W/  rule ==> Seg 1: Sentence <ui>one. Sentence two.</ui>
                
              * Note - using "[-._:A-Za-z0-9]" instead of "\w" to match XML tag names b/c 
                       "-", ".", and ":" are valid and \w doesn't cover them.                
                
              * Note - using "[\s\S]" instead of "." to match any character b/c 
                       "." matches everything except \n and we need to match \n 
                       (see bug 8586)
          -->
          <beforebreak>&lt;([-._:A-Za-z0-9]*?)(?:\b)*.*?&gt;[\s\S]*?</beforebreak>
          <afterbreak>&lt;/\1&gt;</afterbreak>
        </rule>
        <rule break="no">
          <!--Don't break in the middle of self-closing tags. Sometimes these can 
              contain breaking characters in the attribute strings so we need to 
              add this regex as a non-breaker
              e.g. <img alt="Go!" /> 
              
              * Note - using "[-._:A-Za-z0-9]" instead of "\w" to match XML tag names b/c 
                       "-", ".", and ":" are valid and \w doesn't cover them.
          -->
          <beforebreak>&lt;[-._:A-Za-z0-9]*(\s*[-._:A-Za-z0-9]*=\s*("[^"]*"|'[^']*'))*\s*/&gt;</beforebreak>
          <afterbreak></afterbreak>
        </rule>
        <rule break="yes">
          <beforebreak>[\.\?!]+</beforebreak>
          <afterbreak>\s</afterbreak>
        </rule>
        <rule break="yes">
          <beforebreak>\u2029</beforebreak>
          <afterbreak></afterbreak>
        </rule>

        <!--Exclude numeric list bullets. The following example should be considered as one sentence: 
          This is a list: 
          1. Item a, 
          2. Item b, 
          3. The last item. -->
        <rule break="no">
          <beforebreak>^\s*[0-9]+\.</beforebreak>
          <afterbreak>\s</afterbreak>
        </rule>

        <!-- AY added rule for S.M.A.R.T. -->
        <rule break="no">
          <beforebreak>(S\.M\.A\.R\.T\.|S\.M\.A\.R\.T\.\))</beforebreak>
          <afterbreak>\s[^\p{Lu}]</afterbreak>
        </rule>
        <!-- AY -->

        <rule break="no">
          <beforebreak>['"„][\.!?…]['"”]\s</beforebreak>
          <afterbreak></afterbreak>
        </rule>

        <rule break="no">
          <beforebreak>[\[\(]*\.\.\.[\]\)]*</beforebreak>
          <afterbreak>[^\p{Lu}]</afterbreak>
        </rule>

      </languagerule>

      <!-- ByLineBreak -->
      <languagerule languagerulename="ByLineBreak">
        <rule break="yes">
          <beforebreak>\n</beforebreak>
          <afterbreak></afterbreak>
        </rule>
      </languagerule>

      <!-- ByTwoLineBreaks -->
      <languagerule languagerulename="ByTwoLineBreaks">
        <rule break="yes">
          <beforebreak>\n\s*\n[\t]*</beforebreak>
          <afterbreak></afterbreak>
        </rule>
      </languagerule>

      
