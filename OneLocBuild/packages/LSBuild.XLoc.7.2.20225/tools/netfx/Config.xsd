<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:annotation>
        <xs:documentation>This schema defines the allowable content within LOOM Config xml files</xs:documentation>
        <xs:documentation>Version 0.1</xs:documentation>
    </xs:annotation>
    <xs:element name="config">
        <xs:complexType>
            <xs:sequence minOccurs="1">
                <xs:element name="html" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>All items parsed as HTML are defined under this element</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="localizable">
                                <xs:annotation>
                                    <xs:documentation>Localizable HTML items (tags, attributes and properties) are defined under this element</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="tags" />
                                        <xs:element ref="attributes" />
                                        <xs:element ref="properties" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="known">
                                <xs:annotation>
                                    <xs:documentation>Known HTML tags are defined under this element</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="tags" type="knownHTMLTags"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="locked">
                                <xs:annotation>
                                    <xs:documentation>Locked HTML tags are defined under this element</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="tags" type="conditionalTags" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="xml" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>All items parsed as XML are defined under this element</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="localizable">
                                <xs:annotation>
                                    <xs:documentation>Localizable XML items (tags and attributes) are defined under this element</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="tags" />
                                        <xs:element ref="attributes" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="known">
                                <xs:annotation>
                                    <xs:documentation>Known XML (tags, attributes and entities) are defined under this element</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="tags" type="knownXMLTags" />
                                        <xs:element name="attributes" type="knownAttributes" />
                                        <xs:element ref="entities" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="locked">
                                <xs:annotation>
                                    <xs:documentation>Locked XML tags are defined under this element</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="tags" type="conditionalTags" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="options">
                    <xs:annotation>
                        <xs:documentation>All LOOM options are defined under this element</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="general">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:annotation>
                                            <xs:documentation>General LOOM options, each attribute has to be present and contain the defined type</xs:documentation>
                                        </xs:annotation>
                                        <xs:element name="option" maxOccurs="14">
                                            <xs:complexType>
                                                <xs:attribute name="AllowUnderscoresInCSSIdentifiers" type="xs:boolean" default="false" />
                                                <xs:attribute name="AttributeExtractionLevel" type="extractionLevelType" default="ExtractLocalizableItems" />
                                                <xs:attribute name="ContentLocking" type="xs:boolean" default="false" />
                                                <xs:attribute name="CustomValidationDLLs" type="xs:string" default="" />
                                                <xs:attribute name="DisableMessagesForNonLocCSSProperties" type="xs:boolean" default="true" />
                                                <xs:attribute name="DisableXMLValidation" type="xs:boolean" default="false" />
                                                <xs:attribute name="ExtractOnlyTermsWithRealResIds" type="xs:boolean" default="false" />
                                                <xs:attribute name="ExtractScripting" type="xs:boolean" default="true" />
                                                <xs:attribute name="IncludeHiddenAttrValues" type="xs:boolean" default="false" />
                                                <xs:attribute name="LOOMConfigLastModifiedDateTime" type="xs:dateTime" default="0001-01-01T00:00:00" />
                                                <xs:attribute name="PropertyExtractionLevel" type="extractionLevelType" default="ExtractLocalizableItems" />
                                                <xs:attribute name="ReverseLocking" type="xs:boolean" default="false" />
                                                <xs:attribute name="ScriptingResourceModel" type="xs:boolean" default="true" />
                                                <xs:attribute name="ServerSideScripting" type="xs:boolean" default="true" />
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="disabledMessages" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:annotation>
                                            <xs:documentation>Information and Warning messages which are disabled</xs:documentation>
                                        </xs:annotation>
                                        <xs:element name="disabledMessage" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="description" type="xs:string" />
                                                </xs:sequence>
                                                <xs:attribute name="id" type="messageIdType" use="required" />
                                                <xs:attribute name="type" type="messageTypeType" use="required" />
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="fileExtensions">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:annotation>
                                            <xs:documentation>File extensions recognised and the corresponding types, max 12 recognised types</xs:documentation>
                                        </xs:annotation>
                                        <xs:element name="fileExtension" minOccurs="0" maxOccurs="12">
                                            <xs:complexType>
                                                <xs:attribute name="name" type="fileType" use="required" />
                                                <xs:attribute name="extensions" type="extensionsType" use="required" />
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="update">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:annotation>
                                            <xs:documentation>Update specific options, each attribute has to be present and contain the defined type</xs:documentation>
                                        </xs:annotation>
                                        <xs:element name="option" maxOccurs="5">
                                            <xs:complexType>
                                                <xs:attribute name="AdditionCost" type="updateOptionPositivePercentageType" default="3" />
                                                <xs:attribute name="DeletionCost" type="updateOptionPositivePercentageType" default="1" />
                                                <xs:attribute name="MaxFormattingWeight" type="updateOptionPercentageType" default="20" />
                                                <xs:attribute name="MinSimilarity" type="updateOptionPositivePercentageType" default="50" />
                                                <xs:attribute name="MoveCost" type="updateOptionPositivePercentageType" default="2" />
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="tags">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="tag" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:attribute name="name" type="xs:token" use="required" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="conditionalTags">
        <xs:sequence>
            <xs:element name="tag" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="condition" type="xs:string" minOccurs="0" />
                    </xs:sequence>
                    <xs:attribute name="name" type="xs:token" use="required" />
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="knownHTMLTags">
        <xs:sequence>
            <xs:element name="tag" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="locVerRules" type="xs:string" minOccurs="0" />
                    </xs:sequence>
                    <xs:attribute name="name" type="xs:token" use="required" />
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="knownXMLTags">
        <xs:sequence>
            <xs:element name="tag" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="contents" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="content" minOccurs="0" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:simpleContent>
                                                <xs:extension base="xs:string">
                                                    <xs:attribute name="name" type="xs:string" use="required" />
                                                </xs:extension>
                                            </xs:simpleContent>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="locVerRules" type="xs:string" minOccurs="0" />
                    </xs:sequence>
                    <xs:attribute name="name" type="xs:token" use="required" />
                    <xs:attribute name="canBeEmpty" type="xs:boolean" use="optional" default="true" />
                    <xs:attribute name="tagType" type="xmlKnownTagType" use="optional" default="Optional" />
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="attributes">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="tag" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="attribute" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="condition" type="xs:string" minOccurs="0" />
                                        <xs:element name="locVerRules" type="xs:string" minOccurs="0" />
                                    </xs:sequence>
                                    <xs:attribute name="name" type="xs:token" use="required" />
                                    <xs:attribute name="category" type="categoryType" use="required" />
                                    <xs:attribute name="lock" type="xs:boolean" use="optional" default="false" />
                                    <xs:attribute name="type" type="enumerationType" use="optional" />
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attribute name="name" type="xs:token" use="required" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="knownAttributes">
        <xs:sequence>
            <xs:element name="tag" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="attribute" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="values" minOccurs="0">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="value" minOccurs="0" maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:simpleContent>
                                                            <xs:extension base="xs:string">
                                                                <xs:attribute name="name" type="xs:token" use="required" />
                                                            </xs:extension>
                                                        </xs:simpleContent>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="default" type="xs:string" default="" />
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                                <xs:attribute name="name" type="xs:token" use="required" />
                                <xs:attribute name="type" type="enumerationType" use="required" />
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="name" type="xs:token" use="required" />
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="properties">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="property" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="locVerRules" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                        <xs:attribute name="name" type="propertyType" use="required" />
                        <xs:attribute name="category" type="categoryType" use="required" />
                        <xs:attribute name="lock" type="xs:boolean" use="optional" default="false" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="entities">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="entity" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:attribute name="name" type="xs:token" use="required" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:simpleType name="xmlKnownTagType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Optional" />
            <xs:enumeration value="Important" />
            <xs:enumeration value="Required" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="updateOptionPercentageType">
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="-1" />
            <xs:maxInclusive value="100" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="updateOptionPositivePercentageType">
        <xs:restriction base="xs:nonNegativeInteger">
            <xs:maxInclusive value="100" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="messageIdType">
        <xs:annotation>
            <xs:documentation>Message ids range from 1000 to 9998</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:positiveInteger">
            <xs:minInclusive value="1000" />
            <xs:maxExclusive value="9999" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="messageTypeType">
        <xs:annotation>
            <xs:documentation>Message types, only information and warning messages can be disabled</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="Information" />
            <xs:enumeration value="Warning" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="extensionsType">
        <xs:list itemType="xs:string" />
    </xs:simpleType>
    <xs:simpleType name="enumerationType">
        <xs:annotation>
            <xs:documentation>List of recognised content types for known attributes</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="enumeration" />
            <xs:enumeration value="string" />
            <xs:enumeration value="notation" />
            <xs:enumeration value="id" />
            <xs:enumeration value="parentID" />
            <xs:enumeration value="version" />
            <xs:enumeration value="idref" />
            <xs:enumeration value="idrefs" />
            <xs:enumeration value="entity" />
            <xs:enumeration value="entities" />
            <xs:enumeration value="nmtoken" />
            <xs:enumeration value="nmtokens" />
            <xs:enumeration value="boolean" />
            <xs:enumeration value="char" />
            <xs:enumeration value="int" />
            <xs:enumeration value="uri" />
            <xs:enumeration value="bin.base64" />
            <xs:enumeration value="bin.hex" />
            <xs:enumeration value="date" />
            <xs:enumeration value="dateTime" />
            <xs:enumeration value="dateTime.tz" />
            <xs:enumeration value="fixed.14.4" />
            <xs:enumeration value="float" />
            <xs:enumeration value="number" />
            <xs:enumeration value="time" />
            <xs:enumeration value="time.tz" />
            <xs:enumeration value="i1" />
            <xs:enumeration value="i2" />
            <xs:enumeration value="i4" />
            <xs:enumeration value="i8" />
            <xs:enumeration value="r4" />
            <xs:enumeration value="r8" />
            <xs:enumeration value="ui1" />
            <xs:enumeration value="ui2" />
            <xs:enumeration value="ui4" />
            <xs:enumeration value="ui8" />
            <xs:enumeration value="uuid" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="extractionLevelType">
        <xs:annotation>
            <xs:documentation>Extraction level for attributes and properties</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="ExtractLocalizableItems" />
            <xs:enumeration value="ExtractNonLocalizableItems" />
            <xs:enumeration value="ExtractAllItems" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="fileType">
        <xs:annotation>
            <xs:documentation>Recognised file types</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="HTML Files" />
            <xs:enumeration value="CSS Style Sheet Files" />
            <xs:enumeration value="JavaScript Files" />
            <xs:enumeration value="Visual Basic Script Files" />
            <xs:enumeration value="HTML Help Table of Contents Files" />
            <xs:enumeration value="HTML Help Index Files" />
            <xs:enumeration value="Java Files" />
            <xs:enumeration value="XML Files" />
            <xs:enumeration value="XML Stylesheet Files" />
            <xs:enumeration value="RAW Files" />
            <xs:enumeration value="CeX Files" />
            <xs:enumeration value="REG or DEF Files" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="categoryType">
        <xs:annotation>
            <xs:documentation>LocStudio categories for attributes</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="Accelerator" />
            <xs:enumeration value="Address" />
            <xs:enumeration value="Alternative Text" />
            <xs:enumeration value="AppData" />
            <xs:enumeration value="Argument Name" />
            <xs:enumeration value="Argument" />
            <xs:enumeration value="Billboards" />
            <xs:enumeration value="Button" />
            <xs:enumeration value="Check Box" />
            <xs:enumeration value="Combo Box" />
            <xs:enumeration value="Command" />
            <xs:enumeration value="Comment" />
            <xs:enumeration value="ComponentID" />
            <xs:enumeration value="Custom Control" />
            <xs:enumeration value="Description" />
            <xs:enumeration value="Dialog Item" />
            <xs:enumeration value="Dialog Title" />
            <xs:enumeration value="Documentation" />
            <xs:enumeration value="Drive Control" />
            <xs:enumeration value="Edit Box" />
            <xs:enumeration value="Error Message" />
            <xs:enumeration value="Feature Description" />
            <xs:enumeration value="Feature Title" />
            <xs:enumeration value="File Language" />
            <xs:enumeration value="File Name" />
            <xs:enumeration value="File Size" />
            <xs:enumeration value="File version" />
            <xs:enumeration value="Folder Control" />
            <xs:enumeration value="Folder Name" />
            <xs:enumeration value="Font Name" />
            <xs:enumeration value="Function Name" />
            <xs:enumeration value="General" />
            <xs:enumeration value="Group Box Title" />
            <xs:enumeration value="Group Box" />
            <xs:enumeration value="HTML Text" />
            <xs:enumeration value="HTML Title" />
            <xs:enumeration value="INI File Key" />
            <xs:enumeration value="INI File Name" />
            <xs:enumeration value="INI File Section" />
            <xs:enumeration value="INI File Value" />
            <xs:enumeration value="Java Text" />
            <xs:enumeration value="Key Name" />
            <xs:enumeration value="Keyword" />
            <xs:enumeration value="List Box" />
            <xs:enumeration value="Macro Action" />
            <xs:enumeration value="Menu Item" />
            <xs:enumeration value="Message" />
            <xs:enumeration value="Number" />
            <xs:enumeration value="Option Button" />
            <xs:enumeration value="Page Information" />
            <xs:enumeration value="Page Title" />
            <xs:enumeration value="Progress Bar" />
            <xs:enumeration value="Property" />
            <xs:enumeration value="Radio Button" />
            <xs:enumeration value="Registry Value" />
            <xs:enumeration value="Resource Data" />
            <xs:enumeration value="Scroll Bar" />
            <xs:enumeration value="Shortcut Argument" />
            <xs:enumeration value="Shortcut Description" />
            <xs:enumeration value="Shortcut HotKey" />
            <xs:enumeration value="Shortcut Name" />
            <xs:enumeration value="Slider Bar" />
            <xs:enumeration value="Static Text" />
            <xs:enumeration value="Status Bar" />
            <xs:enumeration value="Style" />
            <xs:enumeration value="Tab Control" />
            <xs:enumeration value="Text" />
            <xs:enumeration value="Tool Tip" />
            <xs:enumeration value="Topic Title" />
            <xs:enumeration value="Translation Record" />
            <xs:enumeration value="Unknown" />
            <xs:enumeration value="VB Script Text" />
            <xs:enumeration value="Verb" />
            <xs:enumeration value="Version" />
            <xs:enumeration value="Culture" />
            <xs:enumeration value="Font Size" />
            <xs:enumeration value="Font Style" />
            <xs:enumeration value="Legal" />
            <xs:enumeration value="URI" />
            <xs:enumeration value="Size" />
            <xs:enumeration value="Code" />
            <xs:enumeration value="Adaptation" />
            <xs:enumeration value="Color" />
            <xs:enumeration value="Layout" />
            <xs:enumeration value="Code Page" />
            <xs:enumeration value="Ribbon" />
            <xs:enumeration value="Globalization Data"/>
            <xs:enumeration value="Right to Left order data" />
            <xs:enumeration value="List" />
            <xs:enumeration value="Record" />
            <xs:enumeration value="File Filter" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="propertyType">
        <xs:annotation>
            <xs:documentation>Recognised CSS properties</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="accelerator" />
            <xs:enumeration value="all-space-treatment" />
            <xs:enumeration value="animation" />
            <xs:enumeration value="animation-delay" />
            <xs:enumeration value="animation-direction" />
            <xs:enumeration value="animation-duration" />
            <xs:enumeration value="animation-iteration-count" />
            <xs:enumeration value="animation-name" />
            <xs:enumeration value="animation-play-state" />
            <xs:enumeration value="animation-timing-function" />
            <xs:enumeration value="appearance" />
            <xs:enumeration value="background" />
            <xs:enumeration value="background-attachment" />
            <xs:enumeration value="background-break" />
            <xs:enumeration value="background-clip" />
            <xs:enumeration value="background-color" />
            <xs:enumeration value="background-image" />
            <xs:enumeration value="background-origin" />
            <xs:enumeration value="background-position" />
            <xs:enumeration value="background-position-x" />
            <xs:enumeration value="background-position-y" />
            <xs:enumeration value="background-repeat" />
            <xs:enumeration value="background-size" />
            <xs:enumeration value="behavior" />
            <xs:enumeration value="block-progression" />
            <xs:enumeration value="border" />
            <xs:enumeration value="border-bottom" />
            <xs:enumeration value="border-bottom-color" />
            <xs:enumeration value="border-bottom-style" />
            <xs:enumeration value="border-bottom-width" />
            <xs:enumeration value="border-break" />
            <xs:enumeration value="border-collapse" />
            <xs:enumeration value="border-color" />
            <xs:enumeration value="border-image" />
            <xs:enumeration value="border-left" />
            <xs:enumeration value="border-left-color" />
            <xs:enumeration value="border-left-style" />
            <xs:enumeration value="border-left-width" />
            <xs:enumeration value="border-radius" />
            <xs:enumeration value="border-right" />
            <xs:enumeration value="border-right-color" />
            <xs:enumeration value="border-right-style" />
            <xs:enumeration value="border-right-width" />
            <xs:enumeration value="border-style" />
            <xs:enumeration value="border-top" />
            <xs:enumeration value="border-top-color" />
            <xs:enumeration value="border-top-style" />
            <xs:enumeration value="border-top-width" />
