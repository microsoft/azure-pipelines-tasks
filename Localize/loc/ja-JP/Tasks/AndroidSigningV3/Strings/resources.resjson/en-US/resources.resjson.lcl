      <Item ItemId=";loc.description" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Sign and align Android APK files]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[署名し、Android APK ファイルを配置します]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
