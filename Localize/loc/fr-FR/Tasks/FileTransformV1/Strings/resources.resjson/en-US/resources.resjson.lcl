        <Str Cat="Text">
          <Val><![CDATA[Replace tokens with variable values in XML or JSON configuration files]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[Remplacer les jetons par des valeurs de variables dans les fichiers config XML ou JSON]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
