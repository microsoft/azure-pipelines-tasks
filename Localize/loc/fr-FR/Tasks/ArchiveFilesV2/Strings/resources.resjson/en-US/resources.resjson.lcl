      <Item ItemId=";loc.description" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Compress files into .7z, .tar.gz, or .zip]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[Compresser les fichiers aux formats .7z, .tar.gz ou .zip]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
