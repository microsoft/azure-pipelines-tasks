      <Item ItemId=";loc.description" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Deploy a SQL Server database using DACPAC]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[Implementa una base de datos de SQL Server con DACPAC.]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
