      <Item ItemId=";loc.input.help.comment" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Contents of the comment to be written]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
