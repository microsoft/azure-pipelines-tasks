        <Str Cat="Text">
          <Val><![CDATA[cURL upload files]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[cURL-Uploaddateien]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
