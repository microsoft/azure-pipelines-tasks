      <Item ItemId=";loc.description" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Run shell commands or a script on a remote machine using SSH]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[Esegue i comandi della shell o uno script in un computer remoto usando SSH]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
