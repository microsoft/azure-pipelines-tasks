        <Str Cat="Text">
          <Val><![CDATA[Copy files]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[Copia file]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
