        <Str Cat="Text">
          <Val><![CDATA[PowerShell on Target Machines]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[PowerShell in computer di destinazione]]></Val>
          </Tgt>
        </Str>
