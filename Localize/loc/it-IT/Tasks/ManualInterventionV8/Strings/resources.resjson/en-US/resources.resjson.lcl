      <Item ItemId=";loc.description" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Pause deployment and wait for manual intervention]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[Sospende la distribuzione e attende l'intervento manuale]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
