      "properties": {
        "EditableOptions": "False"
      }
    },
    {
      "name": "Filters",
      "type": "multiLine",
      "label": "ms-resource:loc.input.label.Filters",
      "defaultValue": "",
      "required": true,
      "groupName": "Options",
      "helpMarkDown": "ms-resource:loc.input.help.Filters"
    },
    {
      "name": "RetentionPeriod",
      "type": "int",
