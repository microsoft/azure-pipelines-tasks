      "properties": {
        "EditableOptions": "True"
      }
    },
    {
      "name": "CompositionType",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.CompositionType",
      "defaultValue": "key",
      "required": true,
      "groupName": "Options",
