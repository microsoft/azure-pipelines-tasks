      "type": "string",
      "label": "ms-resource:loc.input.label.vstsPATToken",
      "groupName": "AdvancedDeploymentOptions",
      "visibleRule": "enableDeploymentPrerequisites = ConfigureVMWithMGAgent",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.vstsPATToken"
