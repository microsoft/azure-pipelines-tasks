      "options": {
        "download": "Download",
        "publish": "Publish"
      }
    },
    {
      "name": "downloadDirectory",
      "aliases": [
        "downloadDirectory"
      ],
      "type": "filePath",
      "label": "ms-resource:loc.input.label.downloadDirectory",
