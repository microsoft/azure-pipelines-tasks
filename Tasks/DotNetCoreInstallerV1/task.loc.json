      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.packageType",
      "options": {
        "runtime": "Only Runtime",
        "sdk": "SDK (contains runtime)"
      }
    },
    {
      "name": "version",
      "type": "string",
      "label": "ms-resource:loc.input.label.version",
      "defaultValue": "2.2.x",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.version"
