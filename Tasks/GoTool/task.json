{
    "id": "334727F4-9495-4F9D-A391-FC621D671474",
    "name": "GoTool",
    "friendlyName": "Go Tool Installer",
    "description": "Finds or Downloads and caches specified version spec of Go and adds it to the PATH.",
    "helpMarkDown": "",
    "category": "Tool",
    "runsOn": [
        "Agent",
        "DeploymentGroup"
    ],
    "author": "Microsoft Corporation",
    "version": {
        "Major": 0,
        "Minor": 0,
        "Patch": 1
    },
    "satisfies": ["GO"],
    "demands": [],
    "instanceNameFormat": "Use Go $(version)",
    "inputs": [{
            "name": "version",
            "type": "string",
            "label": "Version",
            "defaultValue": "1.9.2",
            "required": true,
            "helpMarkDown": "Golang tool version to download and install.  Examples: 1.9.2"
        }
    ],
    "execution": {
        "Node": {
            "target": "gotool.js",
            "argumentFormat": ""
        }
    },
    "messages": {
        "ToolFailed": "Tool install failed: %s",
        "VersionNotSupported": "version '%s' is not available"
    }
}