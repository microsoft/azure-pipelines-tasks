      "name": "exportOptions",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.exportOptions",
      "defaultValue": "auto",
      "required": false,
      "groupName": "package",
      "options": {
        "auto": "Automatic",
        "plist": "Plist",
        "specify": "Specify"
      },
      "helpMarkDown": "ms-resource:loc.input.help.exportOptions",
      "visibleRule": "packageApp == true"
