{
  "cache": false,
  "produceSourceMap": false,
  "sourceMap": false,
  "exclude": [
    "**/Tests/**",
    "**/*.d.ts",
    "**/*.spec.*",
    "**/*.test.*",
    "**/*.mock.*",
    "**/test/**",
    "**/tests/**",
    "**/node_modules/**"
  ],
  "watermarks": {
    "lines": [60, 80],
    "functions": [60, 80],
    "branches": [60, 80],
    "statements": [60, 80]
  },
  "reporter": [
    "text",
    "json",
    "json-summary"
  ]
}